{
  "name": "sample-web-application",
  "version": "1.0.0",
  "description": "Here an example to configure your server and how to create your first controller.",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test:lint": "eslint src/**/*.ts",
    "test:lint:fix": "eslint --fix src/**/*.ts",
    "test": "jest",
    "test-with-coverage": "jest --coverage --passWithNoTests",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=prod node dist/index.js",
    "deploy": "exit 0"
  },
  "author": "Dileep Gopalan",
  "license": "MIT",
  "dependencies": {
    "@tsed/common": "5.34.5",
    "@tsed/core": "5.34.5",
    "@tsed/di": "5.34.5",
    "@tsed/swagger": "5.34.5",
    "@tsed/testing": "5.34.5",
    "@types/swagger-schema-official": "2.0.20",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.0.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "cross-env": "6.0.3",
    "dotenv": "8.0",
    "express": "4.17.1",
    "getenv": "0.7",
    "method-override": "^3.0.0",
    "node-uuid": "1.4.8",
    "request": "2.88",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.2",
    "@types/http-proxy": "1.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "12.12.9",
    "@types/request-promise": "4.1.45",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "concurrently": "5.0.0",
    "eslint": "5.16",
    "eslint-config-standard": "12.0",
    "eslint-plugin-import": "2.17",
    "eslint-plugin-node": "9.1",
    "eslint-plugin-promise": "4.1",
    "eslint-plugin-standard": "4.0",
    "husky": "2.4.1",
    "jest": "24.8",
    "lint-staged": "8.2.1",
    "nodemon": "1.19.4",
    "rimraf": "3.0.0",
    "serverless": "1.45",
    "serverless-dotenv-plugin": "2.1.1",
    "serverless-offline": "5.5.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0",
    "ts-node": "8.5.2",
    "typescript": "3.7.2"
  }
}
